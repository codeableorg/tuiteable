<section>
   
    <%@user.likes.order(created_at: :desc).each do |like|%>
        <div>
            <%= image_tag(url_for(@user.avatar)) %>
            <span>@<%= @user.username%> </span>
            <span> <%= like.created_at.strftime("%-d %B")%> </span>
            <p> <%=like.tweet.body%> </p>
            <span><%= like.tweet.comments_count%> </span>
            <span> <%=like.tweet.likes_count%> </span>
        </div>
    <%end%>

</section>