<div class="explorer">
  <h2 class="explorer-title" >Explorer</h2>
</div>
<% if current_user %>
  <div class="tuit">
    <div class="tuit-image">
      <%= image_tag("girl.jpeg" , alt:"girl" ) %>
    </div>
    <div class="tuit-body">
    <%= render 'tuits/tuit_form' %>
    </div>
  </div>
<% end %>  
<div class="tuits">
  <% @tuits.each do |tuit| %>
    <%= render partial:"tuit", locals: { tuit: tuit  } %>
  <% end %>
</div>
